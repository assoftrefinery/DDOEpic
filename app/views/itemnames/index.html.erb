<h1>Listing itemnames</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @itemnames.each do |itemname| %>
  <tr>
    <td><%= itemname.nombre %></td>
    <td><%= link_to 'Show', itemname %></td>
    <td><%= link_to 'Edit', edit_itemname_path(itemname) %></td>
    <td><%= link_to 'Destroy', itemname, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nuevo objeto epico', new_itemname_path %>
